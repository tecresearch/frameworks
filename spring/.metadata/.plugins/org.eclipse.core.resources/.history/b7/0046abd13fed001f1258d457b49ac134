package in.tca.model;

public class Client {

	private String name;
	private Task task;
	@Override
	public String toString() {
		return "Client [name=" + name + ", task=" + task + "]";
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Task getTask() {
		return task;
	}
	public void setTask(Task task) {
		this.task = task;
	}
	
	
}
